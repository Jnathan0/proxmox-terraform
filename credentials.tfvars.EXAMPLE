proxmox_api_url = $PROXMOX_IP  # Your Proxmox IP Address
proxmox_api_token_id = $PROXMOX_API_TOKEN_ID  # API Token ID
proxmox_api_token_secret = $PROXMOX_API_TOKEN_SECRET
agent_maintainer_ssh_pubkey = $AGENT_MAINTAINER_SSH_PUBKEY  # SSH Pubkey of user/maintainer for access
controller_ssh_pubkey = $CONTROLLER_SSH_PUBKEY  # Path to SSH Pubkey of terraform controller for IaC provisioning
controller_ssh_private_key = $CONTROLLER_SSH_PRIVATE_KEY    # Path to SSH Private key of terraform controller for Iac provisioning